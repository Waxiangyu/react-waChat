#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../._sass@1.0.0-beta.5.1@sass/sass.js" "$@"
  ret=$?
else 
  node  "$basedir/../._sass@1.0.0-beta.5.1@sass/sass.js" "$@"
  ret=$?
fi
exit $ret
